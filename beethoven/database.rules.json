{
  "rules": {
    "abrsm": {
      ".read": true,
      ".write": false
    },
    "collections": {
      ".read": true,
      ".write": false
    },
    "composers": {
      ".read": true,
      ".write": false
    },
    "compositions": {
      ".read": true,
      ".write": false
    },
    "key": {
      ".read": true,
      ".write": false
    },
    "form": {
      ".read": true,
      ".write": false
    },
    "rcm": {
      ".read": true,
      ".write": false
    },
    "henle": {
      ".read": true,
      ".write": false
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "user-collections": {
      "$uid": {
        ".read": true,
        ".write": "$uid === auth.uid"
      }
    },
    "user-vanity": {
      ".read": true,
      ".write": "$uid === auth.uid"
    },
    "user-profile": {
      ".read": true,
      ".write": false
    },
    "list-composers": {
      ".read": true,
      ".write": false
    }
  }
}